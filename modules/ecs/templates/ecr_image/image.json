[
    {
      "name": "${container_name}-${environment}",
      "image": "${app_image}",
      "cpu": ${fargate_cpu},
      "memory": ${fargate_memory},
      "networkMode": "awsvpc",
      "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/ecs/deepex-parser-${environment}",
            "awslogs-region": "${aws_region}",
            "awslogs-stream-prefix": "ecs"
          }
      }
    }
]